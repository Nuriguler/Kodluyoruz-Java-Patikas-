public class Work_Page {
    public static void main(String[] args) {
        int n;

        Scanner myObj = new Scanner(System.in);
        System.out.print("Enter The Limit Value :");
        n = myObj.nextInt();

        for (int i = 1; i < n; i*= 4) {
            System.out.println(i);
        }
        System.out.println("//////////");

        for (int i = 1; i < n; i*= 5) {
            System.out.println(i);
        }

    }
}
